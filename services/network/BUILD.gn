copy("copy_prebuilt_network_service") {
  filename = "network_service.mojo"
  if (is_android) {
    sources = [
      "prebuilt/android-arm/$filename",
    ]
  } else {
    assert(is_linux)
    sources = [
      "prebuilt/linux-x64/$filename",
    ]
  }
  outputs = [
    "$root_out_dir/$filename",
  ]
}
